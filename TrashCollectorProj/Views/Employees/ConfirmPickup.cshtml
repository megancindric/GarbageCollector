@model TrashCollectorProj.Models.Customer

@{
    ViewData["Title"] = "ConfirmPickup";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Confirm a Pickup</h1>

<h4>Press submit to confirm pickup</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="ConfirmPickup">
            <div class="form-group">
                <input type="submit" value="Confirm" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to Dashboard</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
